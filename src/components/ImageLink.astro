---
import { Image } from 'astro:assets'
export interface Props {
  href: string
  src: ImageMetadata
  width: number
  height: number
  alt: string
  target?: string
}

const { href, src, width, height, alt, target } = Astro.props
---

<a href={href} target={target}>
  <Image src={src} width={width} height={height} alt={alt} />
</a>
