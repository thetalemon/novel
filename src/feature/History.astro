---
import { HISTORY_LIST } from '../data/history'
---

<ul class='root'>
  {
    HISTORY_LIST.map(({ date, title }) => (
      <li>
        <p>
          {date}: {title}
        </p>
      </li>
    ))
  }
</ul>

<style>
  .card {
    background: rgba(255, 255, 255, 0.8);
    margin: 0 auto;
    font-size: 1.2rem;
    padding: 24px;
    border-radius: 8px;
    color: rgb(55, 55, 55);
  }
</style>
